{
  "name": "What's traffic like?",
  "version": "0.0.5",
  "manifest_version": 2,
  "description": "a quick google traffic checker",
  "background": {"page": "background.html" },
  "browser_action": {
      "name": "Click to open map"
  },
  "manifest_version": 2,
  "icons": { "128": "icon.png" },
  "permissions": ["https://maps.googleapis.com/*","storage","contextMenus","identity"],
  "options_page": "options.html",
  "options_ui": {"page":"options.html", "chrome_style": true},
  "author": "Greg Stupp",
  "content_security_policy": "script-src 'self' https://*.googleapis.com; object-src 'self'"
}
